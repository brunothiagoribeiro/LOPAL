programa {
  funcao inicio() {
  inteiro altura, largura, profundidade, volume
  escreva("Qual é a altura : ")
  leia(altura)
  escreva("Qual é a largura: ")
  leia(largura)
  escreva("E a profundidade: ")
  leia(profundidade)
  volume = altura * profundidade * largura
  escreva("O volume é de: ", volume)    
  }
}
